@using WeiXinPay.Controllers
@{
    Layout = null;
}

<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>ResultNotify</title>
</head>
<body>
    @{
        OutPut output = ViewBag.output;
        if (output.Code != 1)
        {
            <h1>支付失败!</h1>
            <hr />
            <h3>原因: @output.Msg</h3>
        }
        else
        {
            <h1>支付成功!</h1>
            <hr />
            <h3>订单号: @output.Msg</h3>
        }
    }

</body>
</html>
